@model IEnumerable<UCHEBNAYAPRAKTIKA.Models.Street>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.StreetName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Deleted)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City.CityName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StreetName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Deleted)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City.CityName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StreetKey }) |
            @Html.ActionLink("Details", "Details", new { id=item.StreetKey }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StreetKey })
        </td>
    </tr>
}

</table>
